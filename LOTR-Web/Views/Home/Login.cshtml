@model LOTR_Web.Models.ViewModels.LoginViewModel;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "Layout";
}
<div class="form-container">

    <form method="post" asp-action="Login" class="form">
        <span class="tittle ch">Iniciar Sesion</span>
        <input asp-for="Correo" type="email" class="input" placeholder="Correo@example">
        <input asp-for="Contraseña" type="password" class="input" placeholder="Contraseña">
        <button>Iniciar</button>
        <p class="errors">@Html.ValidationSummary(true)</p>
    </form>
</div>